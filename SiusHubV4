local Tab = Window:CreateTab("Teleportation", "11998066503")
local Section = Tab:CreateSection("Players")
local Input = Tab:CreateInput({
	Name = "Teleport to another player",
	PlaceholderText = "Username",
	RemoveTextAfterFocusLost = false,
	Callback = function(User)
        local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
        local p2 = User
        local pos = p1.CFrame
        
        p1.CFrame = game.Players[p2].Character.HumanoidRootPart.CFrame
	end,
})
local Section = Tab:CreateSection("Lobby")
local Button = Tab:CreateButton({
	Name = "SpawnLocation",
	Callback = function()
		local teleport_table = {
            location1 = Vector3.new(0, 6, -7), -- your desired position
         
        }
        
        local tween_s = game:GetService('TweenService')
        local tweeninfo = TweenInfo.new(1,Enum.EasingStyle.Linear)
        
        local lp = game.Players.LocalPlayer
        
        function bypass_teleport(v)
            if lp.Character and 
            lp.Character:FindFirstChild('HumanoidRootPart') then
                local cf = CFrame.new(v)
                local a = tween_s:Create(lp.Character.HumanoidRootPart,tweeninfo,{CFrame=cf})
                a:Play()
                a.Completed:Wait()
                print('Arrived position 0, 6, -7')
                Rayfield:Notify({ -- Nofity the user with Rayfield Nofitication
                Title = "Arrived Position",
                Content = "Successfully arrived position 0, 6, -7 ( SpawnLocation )",
                Duration = 6.5, 
                Image = 11998066503, -- Roblox ID
            }
        )
            end
        end
        
        bypass_teleport(teleport_table.location1)-- The function that takes place when the button is pressed
	end,
})
