local Button = Tab:CreateButton({
	Name = "Victory",
	Callback = function()
		local teleport_table = {
            location1 = Vector3.new(65, 127, -119), -- your desired position
        }
        
        local tween_s = game:GetService('TweenService')
        local tweeninfo = TweenInfo.new(1,Enum.EasingStyle.Linear)
        
        local lp = game.Players.LocalPlayer
        
        function bypass_teleport(v)
            if lp.Character and 
            lp.Character:FindFirstChild('HumanoidRootPart') then
                local cf = CFrame.new(v)
                local a = tween_s:Create(lp.Character.HumanoidRootPart,tweeninfo,{CFrame=cf})
                a:Play()
                a.Completed:Wait()
                print('Arrived position 65, 127, -119')
                Rayfield:Notify({ -- Nofity the user with Rayfield Nofitication
                Title = "Arrived Position",
                Content = "Successfully arrived position 65, 127, -119 ( Victory )",
                Duration = 6.5, 
                Image = 11998066503, -- Roblox ID
            }
        )
            end
        end
        
        bypass_teleport(teleport_table.location1)-- The function that takes place when the button is pressed
	end,
})
